{
    "version": 2,
    "builds": [
      {
        "use": "vercel/static-builder",
        "config": {
          "maxLambdaSize": "5mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/$1",
        "headers": {
          "Cache-Control": "s-maxage=1, stale-while-revalidate"
        }
      }
    ]
  }
  